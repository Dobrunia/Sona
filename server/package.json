{
  "name": "music-service-server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "codegen": "graphql-codegen --config codegen.yml",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma db push",
    "prisma:studio": "prisma studio",
    "s3:cors": "node -e \"import{S3Client,PutBucketCorsCommand}from'@aws-sdk/client-s3';import{config as d}from'dotenv';d();const s=new S3Client({region:process.env.S3_REGION,endpoint:process.env.S3_ENDPOINT,credentials:{accessKeyId:process.env.S3_ACCESS_KEY_ID,secretAccessKey:process.env.S3_SECRET_ACCESS_KEY}});await s.send(new PutBucketCorsCommand({Bucket:process.env.S3_BUCKET,CORSConfiguration:{CORSRules:[{AllowedOrigins:['*'],AllowedMethods:['GET','PUT','HEAD'],AllowedHeaders:['*'],MaxAgeSeconds:3600}]}}));console.log('CORS configured');\""
  },
  "dependencies": {
    "@apollo/server": "^5.4.0",
    "@as-integrations/express4": "^1.1.2",
    "@aws-sdk/client-s3": "^3.716.0",
    "@aws-sdk/s3-request-presigner": "^3.716.0",
    "@prisma/client": "^5.18.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ffprobe-static": "^3.1.0",
    "graphql": "^16.9.0",
    "google-auth-library": "^9.15.0",
    "jsonwebtoken": "^9.0.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.7",
    "@types/ws": "^8.5.10",
    "prisma": "^5.18.0",
    "tsx": "^4.15.5",
    "typescript": "^5.4.5"
  }
}
